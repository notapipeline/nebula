[[inputs.exec]]
commands = [
	"/usr/bin/speedtest -s 2254 -f json-pretty --accept-license --accept-gdpr",
	"/usr/bin/speedtest -s 20672 -f json-pretty --accept-license --accept-gdpr",
	"/usr/bin/speedtest -s 21516 -f json-pretty --accept-license --accept-gdpr",
	"/usr/bin/speedtest -s 38156 -f json-pretty --accept-license --accept-gdpr",
	"/usr/bin/speedtest -s 1695 -f json-pretty --accept-license --accept-gdpr",
	"/usr/bin/speedtest -s 15199 -f json-pretty --accept-license --accept-gdpr",
	"/usr/bin/speedtest -s 40193 -f json-pretty --accept-license --accept-gdpr",
	"/usr/bin/speedtest -s 19146 -f json-pretty --accept-license --accept-gdpr",
	"/usr/bin/speedtest -s 14449 -f json-pretty --accept-license --accept-gdpr",
	"/usr/bin/speedtest -s 23765 -f json-pretty --accept-license --accept-gdpr"
]
name_override = "speedtest"
timeout = "5m"
interval = "10m"
data_format = "json"
tag_keys = [
  "server_name",
  "server_location"
]

json_string_fields = [
  "timestamp",
  "isp",
  "interface_externalIp",
  "server_name",
  "server_host",
  "server_location",
  "server_ip",
  "result_url"
]

